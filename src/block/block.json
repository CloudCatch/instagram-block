{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "cloudcatch/instagram",
	"title": "Instagram",
	"category": "common",
	"description": "Instagram feed.",
	"keywords": [ "instagram" ],
	"textdomain": "default",
	"attributes": {
		"columns": {
			"type": "integer",
			"default": 4
		},
		"carousel": {
			"type": "boolean",
			"default": false
		},
		"infinite": {
			"type": "boolean",
			"default": true
		},
		"centerMode": {
			"type": "boolean",
			"default": false
		},
		"items": {
			"type": "integer",
			"default": 4
		},
		"autoWidth": {
			"type": "boolean",
			"default": false
		},
		"autoHeight": {
			"type": "boolean",
			"default": false
		},
		"carouselJson": {
			"type": "string",
			"default": ""
		}
	},
	"supports": {
		"align": [ "wide", "full" ],
		"html": false,
		"inserter": true,
		"spacing": {
			"blockGap": true,
			"units": [ "px", "em", "rem", "vh", "vw" ],
			"__experimentalDefaultControls": {
				"blockGap": true
			}
		}
	},
	"editorStyle": "cc-instagram-block-editor-css",
	"style": "cc-instagram-style-css",
	"editorScript": "cc-instagram-block-js"
}
